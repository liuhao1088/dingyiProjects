.header {
  width: 100%;
  background-color: tomato;
}

.lbt_div{
  width: 100%;
  height: 750rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.img_div{
  width: 100%;
  border:0rpx white solid;
  border-radius: 12rpx;
  margin-top: 20rpx;
}

.video_div{
  height: 500rpx;
  width: 100%;
  border:0rpx white solid;
  margin-top: 20rpx;
}


.tag_maxdiv {
  width: 90%;
  float: left;
}

.tag {
  height: 120rpx;
  margin-top: 20rpx;
  width: 100%;
  border: 0rpx solid red;
  float: left;
}

.imgdiv {
  width: 100%;
  border-radius: 14rpx;
  border: 0rpx solid red;
  box-shadow: 0 4px 20px 0 rgba(0, 0, 0, 0.2);
  padding-bottom: 40rpx;
  float: left;
}

.img {
  height: 260rpx;
  width: 100%;
  background-color: blue;
  float: left;
  border-top-left-radius: 14rpx;
  border-top-right-radius: 14rpx;
}

.imgdiv_nrdiv {
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  border: 0rpx solid black;
  font-size: 30rpx;
  float: left;
  margin-left: 10rpx;
}

.imgdiv_title {
  width: 90%;
  height: 30rpx;
  line-height: 30rpx;
  border: 0rpx solid black;
  font-size: 24rpx;
  float: left;
  margin-left: 10rpx;
  margin-top: 20rpx;
  color: #999999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.branding {
  width: 100%;
  height: 160rpx;
  margin: auto;
  border-radius: 14rpx;
  background-color: #dfdfdf;
  margin-top: 40rpx;
  float: left;
}

.gcbz_div {
  width: 100%;
  height: 400rpx;
  margin: auto;
  border: 0rpx solid black;
}

.gcbz_nrdiv {
  width: calc(100%/3);
  height: 200rpx;
  border: 0rpx solid red;
  float: left;
  display: flex;
  justify-content: center;
  align-items: center;
}

.gcbz_tagdiv {
  height: 90%;
  border: 0rpx solid black;

}

.gcbz_img {
  height: 65%;
  width: 40%;
  background-color: black;
  float: left;
  margin-left: 50rpx;
}

.gcbz_wz {
  width: 100%;
  font-size: 24rpx;
  line-height: 80rpx;
  float: left;
  text-align: center;
}

.layout {
  width: 100%;
  height: 500rpx;
  border: 0rpx solid red;
  float: left;
}

.layout_maxdiv {
  width: 100%;
  height: 170rpx;
  border: 0rpx solid black;
  float: left;
  margin-top: 40rpx;
}

.layout_imgdiv {
  position: relative;
  height: 100%;
  width: 40%;
  border-radius: 10rpx;
  background-color: #2C4E67;
  float: left;
}

.layout_wz {
  height: 20%;
  width: 21%;
  text-align: center;
  color: white;
  border: 1rpx solid white;
  background-color: #2F2F2F;
  position: relative;
  top: 70%;
  left: 70%;
  font-size: 18rpx;
  line-height: 32rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}

.layout_zhongdiv {
  width: 56%;
  height: 100%;
  float: right;

}

.layout_title {
  width: 100%;
  float: left;
  line-height: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.layout_date {
  float: left;
  color: #999999;
  font-size: 20rpx;
  line-height: 20rpx;
  margin-top: 10rpx;

}

.tab_img {
